# CatAttack Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API (for attacker, target_model, judge)
OPENAI_API_KEY=your-openai-api-key-here

# Fireworks API (for proxy_target - optional)
FIREWORKS_API_KEY=your-fireworks-api-key-here

# Anthropic API (optional, if using Claude models)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# HuggingFace Token (optional, for pushing datasets to Hub)
HF_TOKEN=your-huggingface-token-here

